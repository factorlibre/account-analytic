<?xml version="1.0" encoding="utf-8"?>
<openerp>
    <data>
        <record id="view_pos_config_form" model="ir.ui.view">
            <field name="name">pos.config.form.view (pos_analytic_by_config)</field>
            <field name="model">pos.config</field>
            <field name="inherit_id" ref="point_of_sale.pos_config_view_form"/>
            <field name="arch" type="xml">
                <xpath expr="//div[@id='payment_methods']" position="after">
                    <div class="col-xs-12 col-md-6 o_setting_box" id="account_analytic">
                        <div class="o_setting_right_pane">
                            <label string="Analytic Account"/>
                            <div class="text-muted">
                                Analytic Account
                            </div>
                            <div class="content-group mt16">
                                <field name="account_analytic_id" colspan="4" nolabel="1"/>
                            </div>
                        </div>
                    </div>
                </xpath>
            </field>
        </record>
    </data>
</openerp>
